<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: #393939;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxmlapplication.FXMLMainPageController">
   <center>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #373737;" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </center>
   <left>
      <VBox prefHeight="1020.0" prefWidth="300.0" style="-fx-border-color: red; -fx-border-width: 0 3 0 0;" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #303030;" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="75.0" prefWidth="300.0">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="MENU">
                           <font>
                              <Font name="Ebrima Bold" size="36.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="20.0" top="20.0" />
                           </VBox.margin>
                        </Text>
                        <HBox prefHeight="33.0" prefWidth="300.0">
                           <children>
                              <Button fx:id="editCategory" mnemonicParsing="false" prefHeight="46.0" prefWidth="300.0" style="-fx-background-color: #323232;" text="Edit Category" textFill="#b9b9b9">
                                 <font>
                                    <Font name="Ebrima Bold" size="18.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                           </children>
                        </HBox>
                        <HBox prefHeight="33.0" prefWidth="300.0">
                           <children>
                              <Button fx:id="print" mnemonicParsing="false" prefHeight="46.0" prefWidth="300.0" style="-fx-background-color: #303030;" text="Print" textFill="#b9b9b9">
                                 <font>
                                    <Font name="Ebrima Bold" size="18.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="editProfile" mnemonicParsing="false" style="-fx-background-color: #393939;" text="ProfileName" textFill="WHITE">
                     <graphic>
                        <ImageView fitHeight="65.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/profile.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Ebrima Bold" size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </left>
   <top>
      <StackPane fx:id="topStackPane" maxHeight="-Infinity" prefHeight="60.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="topHBox" prefHeight="100.0" prefWidth="200.0">
               <effect>
                  <BoxBlur height="0.0" iterations="3" width="11.0" />
               </effect>
            </HBox>
            <HBox alignment="TOP_CENTER" maxHeight="-Infinity" prefHeight="60.0" prefWidth="200.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="ExpenseTrack">
                           <font>
                              <Font name="Ebrima Bold" size="18.0" />
                           </font>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Text>
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="bCancel" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: #303030;" textFill="WHITE">
                           <graphic>
                              <ImageView fitHeight="45.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/cross.png" />
                                 </image>
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                              </ImageView>
                           </graphic>
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <HBox.margin>
                              <Insets right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </StackPane>
   </top>
   <right>
      <StackPane fx:id="rightStackPane" prefHeight="150.0" prefWidth="600.0" style="-fx-background-color: #303030;" BorderPane.alignment="CENTER">
         <children>
            <VBox fx:id="vBoxEC" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: #a20000; -fx-border-width: 0 0 0 3;">
               <children>
                  <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#cancelStackPAneButtonAction" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: #303030;">
                     <graphic>
                        <ImageView accessibleRole="BUTTON" fitHeight="26.0" fitWidth="23.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/cross.png" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit Category">
                           <font>
                              <Font name="Ebrima Bold" size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets top="10.0" />
                           </HBox.margin>
                        </Text>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="vBoxProfile" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: #a20000; -fx-border-width: 0 0 0 3;">
               <children>
                  <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#cancelStackPAneButtonAction" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: #303030;">
                     <graphic>
                        <ImageView accessibleRole="BUTTON" fitHeight="26.0" fitWidth="23.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/cross.png" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Profile">
                           <font>
                              <Font name="Ebrima Bold" size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets top="10.0" />
                           </HBox.margin>
                        </Text>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </right>
</BorderPane>
